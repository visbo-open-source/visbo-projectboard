<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui">
    <ribbon>
        <tabs>
          <tab id="Tab2" label="VISBO Report Creator">
            <group id="groupCreation" label="Report Creation">
              <button id="idcreateReports" onAction="createReports" label="Create Report" />
              <button id="idcreatePortfolioReports" onAction="createPortfolioReports" label="Create Portfolio Report" />
            </group>
            <group id="groupGeneral" label="General">
              <button id="idprotectReport" onAction="protectReport" label="Protect Report" />
              <button id="idcreateLegend" onAction="createLegend" label="Create Legend" />
              <button id="idcpSettings" onAction="cpsettings" label="Settings" />
            </group>
          </tab>
        </tabs>
    </ribbon>
</customUI>
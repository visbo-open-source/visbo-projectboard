            End If

            anzahlCurrentSlides = pptCurrentPresentation.Slides.Count
            pptSlide = pptCurrentPresentation.Slides(anzahlCurrentSlides)

            ' jetzt muss die Slide als smartSlide gekennzeichnet werden 

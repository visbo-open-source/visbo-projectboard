<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon startFromScratch="true">
    <contextualTabs>
      <tabSet idMso="TabSetChartTools" visible="false" />
      <tabSet idMso="TabSetDrawingTools" visible ="false" />
      <tabSet idMso="TabSetPictureTools" visible ="false" />
      <tabSet idMso="TabSetSmartArtTools" visible ="false" />
    </contextualTabs>
    <!--
    
    -->
    <tabs>
      <tab id="PTX" label ="Management-Info">
        <group id="PTXG1" label ="Schnell-Analyse">
          <button id="PTXG1B1" label="Projekt-Bewertung" onAction="Tom2G2M5M1B3ShowStatus" />
          <separator id="PTXG1s1"/>
          <button id="PTXG1B2" label="Ziele-Bewertung" onAction="PT0ShowZieleUebersicht" />
          <separator id="PTXG1s2"/>
          <!--<button id="PTXG1B3" label="alle roten Meilensteine" onAction="Tom2G2M5M2B3ShowMilestones" />
          <separator id="PTXG1s3"/>
          <button id="PTXG1B4" label="Auslastung" onAction="PT0ShowAuslastung" />
          <separator id="PTXG1s4"/>
          <button id="PTXG1B5" label="Engpass" onAction="PTXShowEngpass" />
          <separator id="PTXG1s5"/>-->
          <button id="PTXG1B6" label="Symbole löschen" onAction="Tom2G2M5B3NoShowSymbols" />
        </group>
      </tab>
      <!--
      
      -->
      <tab id="PT0" label="Informieren">
        <group id="PT0G1" label="Projekt">
          <button id="PT0G1B1" label="Status" onAction="PT0ShowProjektStatus" />
          <separator id="PT0G1s1"/>
          <menu id="PT0G1M0" label="Phasen">
            <button id="PT0G1M0B1" label="Einzelne Phasen" onAction="PT0VisualizePhases" />
            <button id="PT0G1M0B2" label="Phasen Übersicht" onAction="Tom2G2M1B1Phasen" />
          </menu>
          <separator id="PT0G1s2"/>
          <menu id="PT0G1M1" label="Ressourcen">
            <button id="PT0G1M1B1" label="Personal Bedarfe" onAction="Tom2G2M1B2Resources" />
            <button id="PT0G1M1B2" label="Kosten Bedarfe" onAction="Tom2G2M1B5GKosten" />
          </menu>
          <separator id="PT0G1s3"/>
          <menu id="PT0G1M2" label="Meilensteine">
            <button id="PT0G1M2B1" label="Grün" onAction="Tom2G2M5M2B1ShowMilestones" />
            <button id="PT0G1M2B2" label="Gelb" onAction="Tom2G2M5M2B2ShowMilestones" />
            <button id="PT0G1M2B3" label="Rot" onAction="Tom2G2M5M2B3ShowMilestones" />
            <button id="PT0G1M2B4" label="Grau" onAction="Tom2G2M5M2B4ShowMilestones" />
            <button id="PT0G1M2B5" label="Alle" onAction="Tom2G2M5M2B5ShowMilestones" />
            <button id="PT0G1M2B6" label="Namen wählen" onAction="PTShowMilestonesByName" />
            <button id="PT0G1M2B7" label="Symbole löschen" onAction="Tom2G2M5B3NoShowMilestones" />
          </menu>
          <separator id="PT0G1s4"/>
          <button id="PT0G1B3" label="Strategie / Risiko" onAction="Tom2G2M1B6SFIT" />
          <button id="PT0G1B4" label="Komplexität / Risiko" onAction="Tom2G2M1B6CRisk" />
          <separator id="PT0G1s5"/>
          <button id="PT0G1B5" label="Ergebnis" onAction="Tom2G2M1B7Ergebnis" />
        </group>
        <!---->
        <group id="PT0G2" label="Portfolio">
          <button id="PT0G2B1" label="Status" onAction="Tom2G2M5M1B3ShowStatus" />
          <separator id="PT0G2s1"/>
          <menu id="PT0G2M0" label="Phasen">
            <button id="PT0G2M0B1" label="Phasen visualisieren" onAction="PT0VisualizePhasesAll" />
            <button id="PT0G2M0B2" label="Summen Charts" onAction="PT0ShowPortfolioPhasen" />
          </menu>
          <separator id="PT0G2s2"/>
          <menu id="PT0G2M1" label="Ressourcen">
            <button id="PT0G2M1B1" label="Auslastungs-Übersicht" onAction="PT0ShowAuslastung" />
            <button id="PT0G2M1B2" label="Summen Charts Rolle" onAction="PT0ShowPersonalBedarfe" />
            <button id="PT0G2M1B3" label="Summen Charts Kostenart" onAction="PT0ShowKostenBedarfe" />
          </menu>
          <separator id="PT0G2s3"/>
          <menu id="PT0G2M2" label="Meilensteine">
            <button id="PT0G2M2B1" label="Übersicht" onAction="PT0ShowZieleUebersicht" />
            <button id="PT0G2M2B2" label="Grün" onAction="Tom2G2M5M2B1ShowMilestones" />
            <button id="PT0G2M2B3" label="Gelb" onAction="Tom2G2M5M2B2ShowMilestones" />
            <button id="PT0G2M2B4" label="Rot" onAction="Tom2G2M5M2B3ShowMilestones" />
            <button id="PT0G2M2B5" label="Grau" onAction="Tom2G2M5M2B4ShowMilestones" />
            <button id="PT0G2M2B6" label="Alle" onAction="Tom2G2M5M2B5ShowMilestones" />
            <button id="PT0G2M2B7" label="Namen wählen" onAction="PTShowMilestonesByName" />
            <button id="PT0G2M2B8" label="Symbole löschen" onAction="Tom2G2M5B3NoShowMilestones" />
          </menu>
          <separator id="PT0G2s4"/>
          <button id="PT0G2B3" label="Strategie / Risiko" onAction="PT0ShowStrategieRisiko" />
          <button id="PT0G2B4" label="Komplexität / Risiko" onAction="PT0ShowComplexRisiko" />
          <button id="PT0G2B6" label="Zeit / Risiko" onAction="PT0ShowZeitRisiko" />
          <separator id="PT0G2s5"/>
          <button id="PT0G2B5" label="Ergebnis" onAction="PT0ShowPortfolioErgebnis" />
        </group>
      </tab>
      <!--
      
      -->
      <tab id="PT1" label="Reports erstellen">
        <group id="PT1G1" label="Powerpoint">
          <button id="PT1G1B1" label="Einzelprojekt Bericht" onAction="Tom2G1Doku" />
          <separator id="PT1G1s1"/>
          <button id="PT1G1B2" label="Portfolio Bericht" onAction="awinAllprojectsReport" />
        </group>
      </tab>
      <!--
      
      -->
      <tab id ="PT2" label="Bearbeiten">
        <group id="PT2G1" label="Projekt">
          <button id="PT2G1B0" label="Neu" onAction="PT2ProjektNeu" />
          <separator id="PT2G1s0"/>
          <menu id="PT2G1M1" label="Editieren">
            <button id="PT2G1M1B1" label="Ressourcen Bedarfe" onAction="Tom2G1Resources" />
            <button id="PT2G1M1B2" label="Strategie/Risiko/Budget" onAction="Tom2G1Attribute" />
          </menu>
          <separator id="PT2G1s3"/>
          <button id="PT2G1B1" label="Beauftragen/Freigeben" onAction="Tom2G1Beauftragen" />
          <button id="PT2G1B2" label="Freigabe aufheben" onAction="Tom2GXBeauftragen" />
          <separator id="PT2G1s1"/>
          <button id="PT2G1B3" label="in Warteschlange stellen" onAction="Tom2G1NoShow" />
          <button id="PT2G1B4" label="aus Warteschlange holen" onAction="Tom2G1Show" />
          <separator id="PT2G1s2"/>
          <button id="PT2G1B5" label="Umbenennen" onAction="Tom2G1Rename" />
          <separator id="PT2G1s4"/>
          <button id="PT2G1B6" label="Löschen" onAction="Tom2G1Loeschen" />
        </group>
        <!---->
        <group id="PT2G2" label="Portfolio">
          <button id="PT2G2B1" label="Neu/Speichern" onAction="awinNeueKonstellation" />
          <separator id="PT2G2s1"/>
          <button id="PT2G2B2" label="Laden" onAction="awinLadenKonstellation" />
          <separator id="PT2G2s2"/>
          <button id="PT2G2B3" label="Löschen" onAction="awinTestNewFunctions" />
        </group>
      </tab>
      <!--
      
      -->
      <tab id ="PT3" label="Vergleichen">
        <group id="PT3G1" label="Projekt">
          <!--<button id="PT3G1B0" label="Phasen vergleichen in 1 Chart" onAction="Tom2G3M1B1PhasenVgl" />-->
          <button id="PT3G1B1" label="mit Projekt bzw. Vorlage vergleichen" onAction="Tom2G3M1B2PhasenVgl" />
          <button id="PT3G1B2" label="mit letztem Stand vergleichen" onAction="PT3G1B2PhasenVgl" />
          <separator id="PT3G1s1"/>
          <button id="PT3G1B3" label="Ressourcen vergleichen" onAction="Tom2G3M1B2ResourceVgl" />
          <separator id="PT3G1s2"/>
          <menu id="PT3G1M1" label ="Soll/Ist">
            <button id="PT3G1M1B1" label="Personalkosten" onAction="Tom2G2M2M1B2SollIstPKosten" />
            <button id="PT3G1M1B2" label="Sonstige Kosten" onAction="Tom2G2M2M2B2SollIstAKosten" />
            <button id="PT3G1M1B3" label="Gesamt Kosten" onAction="Tom2G2M2M3B2SollIstGKosten" />
          </menu>
          <separator id="PT3G1s3"/>
          <button id="PT3G1B4" label="Time-Machine" onAction="awinShowTimeMachine"/>
          <separator id="PT3G1s4"/>
          <menu id="PT3G1M2" label="Projekt Historie">
            <button id="PT3G1M2B1" label="Strategie/Risiko" onAction="awinShowTrendSR"/>
            <button id="PT3G1M2B2" label="Kennzahlen" onAction="awinShowTrendKPI"/>
          </menu>
        </group>
        <!---->
        <group id="PT3G2" label="Portfolio">
          <button id="PT3G2B1" label="Vergleichen" onAction="awinSetModusHistory" />
        </group>
      </tab>
      <!--
      
      -->
      <tab id ="PT4" label="Austauschen">
        <group id="PT4G1" label=" ">
          <button id="PT4G1B1" label="Import Projekt-Inventur" onAction="Tom2G4B1InventurImport" />
          <separator id="PT4G1s1"/>
          <menu id="PT4G1M1" label ="Projekt Steckbriefe">
            <button id="PT4G1B2" label="Importieren" onAction="Tom2G4M1Import" />
            <button id="PT4G1B3" label="Exportieren" onAction="Tom2G4M1Export" />
          </menu>
          <separator id="PT4G1s2"/>
          <button id="PT4G1B4" label="Export Rollen-Zuordnung" onAction="Tom2G4M2B2ZuordnungRP" />
        </group>

      </tab>
      <tab id ="PT5" label ="Einstellungen">
        <group id ="PT5G1" label ="Projekt-Form">
          <!--<toggleButton id="PT5G1B1" label="Phasen zeichnen" getPressed="PT5phasenZeichnenInit" onAction ="PT5phasenZeichnen"/>-->
          <checkBox id="PT5G1B1" label="Phasen zeichnen" getPressed="PT5phasenZeichnenInit" onAction ="PT5phasenZeichnen"/>
        </group>
        <group id ="PT5G2" label ="Zeitraum">
          <!--<toggleButton id="PT5G2B1" label="bei Veränderung Projekte nachladen" getPressed="PT5loadprojectsInit" onAction ="PT5loadProjectsOnChange"/>-->
          <checkBox id="PT5G2B1" label="bei Veränderung Projekte nachladen" getPressed="PT5loadprojectsInit" onAction ="PT5loadProjectsOnChange"/>
          <button id="Pt5G2B2" label="Zeitraum ändern" onAction="PT5changeTimeSpan" />
        </group>
      </tab>
     </tabs>
  </ribbon>
</customUI>


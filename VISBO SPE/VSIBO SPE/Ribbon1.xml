<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load" loadImage="getImage">
	<commands>
		<!-- Deaktiviert die Hilfe -->
		<command idMso="Help" enabled="false"/>
		<!-- Deaktiviert das Schließkreuz -->
		<command idMso="WindowClose" enabled="false"/>
		<!-- Deaktiviert "Wiederherstellen" -->
		<command idMso="WindowRestore" enabled="false"/>
		<!-- Deaktiviert "Minimieren" -->
		<command idMso="WindowMinimize" enabled="false"/>
		<!-- Deaktiviert das Optionsmenü -->
		<command idMso="ApplicationOptionsDialog" enabled="true"/>
		<!-- Deaktiviert die Beenden-Schaltfläche -->
		<command idMso="FileExit" enabled="false" />
	</commands>
	<ribbon startFromScratch="true">
		<contextualTabs>
			<tabSet idMso="TabSetChartTools" visible="false" />
			<tabSet idMso="TabSetDrawingTools" visible ="false" />
			<tabSet idMso="TabSetPictureTools" visible ="false" />
			<tabSet idMso="TabSetSmartArtTools" visible ="false" />
		</contextualTabs>
		<tabs>
			<tab id="PTVISBO" label="VISBO">
				<group id ="PT6G6" label="VISBO Simple Project Edit">
					<button id="Pt6G6B0" label="Load" size ="large" imageMso="MindMapExportData" onAction="PTProjectLoad" />
					<button id="Pt6G6B1" label="Save" size ="large" imageMso="MindMapImportData"  onAction="PTProjectSave" />
					<!--<button id="Pt6G6B2" label="Unload" visible="true" size ="large" imageMso="Delete" onAction="PTProjectDelete" />-->
					<separator id="Pt6G6B0s1"/>
					<button id="Pt6G6B3" label="Cost" size ="large"  getImage="imageSuper_GetImage" onAction="PTProjectCost" supertip="The costs for the selected projects can be edited here"/>
					<button id="Pt6G6B5" label="Resources" size ="large"  getImage="imageSuper_GetImage" onAction="PTProjectResources"  supertip ="The resource requirements for the selected projects can be edited here"/>
					<button id="Pt6G6B4" label="Time" size ="large"   getImage="imageSuper_GetImage" onAction="PTProjectTime" supertip="The beginning and the end for the phases and milestones of the selected projects can be edited here"/>
					<separator id="Pt6G6B0s2"/>
          <button id="Pt6G6B8" label="Load Context" size ="large"   getImage="imageSuper_GetImage" onAction="PTLoadPiC_ConstellationFromDB" />
          <button id="Pt6G6B9" label="Show Charts" size ="large"   getImage="imageSuper_GetImage" onAction="PTSPEshowCharts" />
					<separator id="Pt6G6B0s3"/>
          <button id="Pt6G6B6" label="WebUI" size ="large"   getImage="imageSuper_GetImage" onAction="PTProjectGoToWebUI" />
          <separator id="Pt6G6B0s4"/>
          <button id="Pt6G6B7" label="Settings"  size ="large"  getImage="imageSuper_GetImage"  onAction="PTProjectEditSettings" supertip="settings which affect resource allocation when changes occur"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>